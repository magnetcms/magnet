{
  "name": "@magnet-cms/admin",
  "version": "0.1.2",
  "private": false,
  "type": "module",
  "main": "./dist/lib/index.cjs",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.js"
      },
      "require": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/lib/index.cjs"
      }
    },
    "./styles": {
      "import": "./dist/lib/styles.css",
      "require": "./dist/lib/styles.css"
    },
    "./styles.css": "./dist/lib/styles.css"
  },
  "files": [
    "dist/lib",
    "dist/client"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "VITE_BASE_PATH=/ vite",
    "build": "vite build",
    "build:standalone": "VITE_BASE_PATH=/ vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:all": "bun run build && bun run build:lib"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": false
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@magnet-cms/ui": "^0.1.0",
    "@magnet-cms/utils": "^0.1.0",
    "react-hook-form": "^7.54.2",
    "ts-pattern": "^5.6.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.9",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/react-query": "^5.67.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3",
    "vite": "^5.2.6",
    "vite-plugin-dts": "^4.5.0"
  },
  "keywords": [
    "magnet",
    "admin",
    "cms",
    "react",
    "nestjs",
    "content-management"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magnet/magnet.git",
    "directory": "packages/client/admin"
  },
  "license": "MIT"
}
